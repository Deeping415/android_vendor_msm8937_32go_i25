tc 1
#Speaker Device
!Playback
Rxdevice:0

enable
QUAT_MI2S_RX Audio Mixer MultiMedia1:1
fs16xx Left preset mode:0

disable
fs16xx Left preset mode:1
QUAT_MI2S_RX Audio Mixer MultiMedia1:0

tc 2
# DMIC1 to Speakers AFE loopback
!AfeLoop
Txdevice:11
Rxdevice:25

enable
QUAT_MI2S_RX Port Mixer TERT_MI2S_TX:1
DEC1 MUX:DMIC1
QUAT_MI2S_RX_DL_HL Switch:1
aw8896_speaker_switch:On

disable
QUAT_MI2S_RX Port Mixer TERT_MI2S_TX:0
DEC1 MUX:ZERO
QUAT_MI2S_RX_DL_HL Switch:0
aw8896_speaker_switch:Off

tc 3
# DMIC2 to Speakers AFE loopback
!AfeLoop
Txdevice:11
Rxdevice:25

enable
QUAT_MI2S_RX Port Mixer TERT_MI2S_TX:1
DEC1 MUX:DMIC2
QUAT_MI2S_RX_DL_HL Switch:1
aw8896_speaker_switch:On

disable
QUAT_MI2S_RX Port Mixer TERT_MI2S_TX:0
DEC1 MUX:ZERO
QUAT_MI2S_RX_DL_HL Switch:0
aw8896_speaker_switch:Off

tc 4
#DMIC1
!Capture

enable
MultiMedia1 Mixer TERT_MI2S_TX:1
DEC1 MUX:DMIC1

disable
DEC1 MUX:ZERO
MultiMedia1 Mixer TERT_MI2S_TX:0

tc 5
#DMIC2
!Capture

enable
MultiMedia1 Mixer TERT_MI2S_TX:1
DEC1 MUX:DMIC2

disable
DEC1 MUX:ZERO
MultiMedia1 Mixer TERT_MI2S_TX:0

tc 8
#Speaker Device
!Playback
Rxdevice:0

enable
QUAT_MI2S_RX Audio Mixer MultiMedia1:1
fs16xx Left preset mode:0
RX3 Digital Volume:67

disable
QUAT_MI2S_RX Audio Mixer MultiMedia1:0
fs16xx Left preset mode:1


tc 243
#Speaker to DMIC1 External loopback
!ExternalLoop
Rxdevice:0
Txdevice:0

enable
QUAT_MI2S_RX Audio Mixer MultiMedia1:1
fs16xx Left preset mode:0
RX3 Digital Volume:67

MultiMedia1 Mixer TERT_MI2S_TX:1
DEC1 MUX:DMIC1
IIR1 INP1 MUX:DEC1

disable
QUAT_MI2S_RX Audio Mixer MultiMedia1:0
DEC1 MUX:ZERO
MultiMedia1 Mixer TERT_MI2S_TX:0
